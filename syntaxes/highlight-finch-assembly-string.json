{
  "scopeName": "python-finch-asm.injection",
  "fileTypes": [
    "py"
  ],
  "injectionSelector": "L:string.quoted.multi.python",
  "patterns": [
    {
      "begin": "\\G(finch|finch-asm)\n",
      "end": "(?=\"\"\"|''')",
      "captures": {
        "1": {
          "name": "keyword.custom"
        }
      },
      "patterns": [
        { "include": "#comments" },
        { "include": "#keywords" },
        { "include": "#builtins" },
        { "include": "#parenthesis" },
        { "include": "#numbers" },
        { "include": "#variables" },
        { "include": "#operators" }
      ]
    }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.line.double-slash.simple",
          "match": "//.*$"
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.simple",
          "match": "\\b(for|in|end)\\b"
        },
        {
          "name": "keyword.operator.range.simple",
          "match": ":"
        }
      ]
    },
    "builtins": {
      "patterns": [
        {
          "name": "support.function.builtin.python",
          "match": "\\bresize\\b"
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.simple",
          "match": "\\b[0-9]+\\b"
        }
      ]
    },
    "variables": {
      "patterns": [
        {
          "name": "variable.other.simple",
          "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
        }
      ]
    },
    "operators": {
      "patterns": [
        {
          "name": "keyword.operator.assignment.simple",
          "match": "=|\\*|\\+|,"
        }
      ]
    },
    "parenthesis": {
      "patterns": [
        {
          "name": "keyword.custom",
          "match": "\\(|\\)|\\[|\\]"
        }
      ]
    }
  }
}
